  <%- contentFor('body') %>
  
  <section id="aa-slider" class="<%= (page == 'kost') ? 'property-kost-page' : 'property-page' %>">
    <div class="img-place">
        <% if(page == 'kost') {
        %>
            <img src="default/img/header-kost.png">
            <%
        } else if(page == 'kontrakan') {
        %>
            <img src="default/img/header-kontrakan.png">
            <%
        } else {
        %>
            <img src="default/img/header-apartemen.png">
        <%
        }%>
    </div>
    <div class="container">
        <div class="text-layout">
            <h1 style="color: #000;">Cari <span style="color: #C58E24;"><%= page %></span></h1>
            <h1 style="color: #000;">di sekitar kamu!</h1>
        </div>
    </div>
    <div class="search">
        <div class="container-fluid">
            <form action="javascript:void(0)" method="get">
                <div class="row">
                    <div class="col-lg-4 col-md-4">
                        <div class="form-group">
                            <label for="">Lokasi</label>
                            <select name="" class="form-control">
                                <option value="" selected hidden>-- Nama Daerah/Kampus --</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-2 col-md-2">
                        <div class="form-group">
                            <label for="">Waktu</label>
                            <select name="" class="form-control">
                                <option value="" selected hidden>-- Waktu --</option>
                                <option value="6">6 Bulan</option>
                                <option value="12">12 Bulan</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-2 col-md-2">
                        <div class="form-group">
                            <label for="">Kamar</label>
                            <select name="" class="form-control">
                                <option value="" selected hidden>-- Kamar --</option>
                                <option value="2">2 Kamar</option>
                                <option value="3">3 Kamar</option>
                                <option value="4">4 Kamar</option>
                                <option value="5">5+ Kamar</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-3">
                        <div class="form-group">
                            <label for="">Harga</label>
                            <input type="number" name="" class="form-control" placeholder="0"/>
                            -
                            <input type="number" name="" class="form-control" placeholder="100.000"/>
                        </div>
                    </div>
                    <div class="col-lg-1 col-md-1">
                        <div class="form-group" style="background-color: transparent !important; padding-right: 0; padding-left: 0;">
                            <button class="btn btn-dark btn-rounded"><i class="fa fa-search"></i> Cari</button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
  </section>

  <section id="service" style="background-color: #F4F4F4;">
    <div class="container">
        <div class="breadcumb">
            <div class="count">
                267 Properti
            </div>
            <div class="filter">
                Urutkan sesuai: <span><a href="javascript:void(0)"> Populer <i class="fa fa-caret-down"></i></a></span>
            </div>
        </div>
    </div>
  </section>

